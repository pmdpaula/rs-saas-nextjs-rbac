{
  "extends": "@saas/ts-config/node.json",
  "include": [
    "src/**/*",
    "@types/*.d.ts",
    "prisma/seed.ts"
  ],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "@http/*": [
        "./src/http/*"
      ],
      "@lib/*": [
        "./src/lib/*"
      ],
    }
  }
}
